#!/bin/bash
git reflog | awk '{
    print "     /@@@@@@@@@@@@@@@@@@@@@@                               ";
    print $6"                     @@@@@                             ";
    print "     \@@@@@@@@@@@@@@@@     @@@@@                        ";
    print "                  @@@@@@     @@@@@                      ";
    print "                     @@@@@     @@@@@                    ";
    print "                       @@@@@     @@@@@                 ";
    print "                         @@@@@     @@@@@@@@@@@@@@@@@\  ";
    print "                           @@@@@                     "$8;
    print "                             @@@@@@@@@@@@@@@@@@@@@@@/  ";
    exit }'

                                                                                
                                                                                
                                                                                